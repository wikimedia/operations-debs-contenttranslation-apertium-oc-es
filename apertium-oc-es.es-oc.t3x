    <rule comment="CHUNK: arregla en cv de relatiu que/qui">
      <pattern>
	<pattern-item n="qui"/>
      </pattern>
      <action>
	<out>
	  <lu>
	    <lit v="qui"/>
	    <clip pos="1" part="a_rel"/>
	    <clip pos="1" part="gen"/>
	    <clip pos="1" part="nbr"/>
	  </lu>
	</out>   
      </action>
    </rule> 

    <rule comment="CHUNK: verb (canvia alguns lemes de verb)">
      <pattern>
	<pattern-item n="verb"/>
      </pattern>
      <action>
	<let>
	  <var n="quin_verb"/>
	  <clip pos="0" part="lema_vb"/>
	</let>
	<call-macro n="cv_verb">
	  <with-param pos="1"/>
	</call-macro>
	
	<out>
	  <lu>
	    <!--  					<var n="quin_verb"/>  -->
	    <clip pos="1" part="whole"/>
	    
	  </lu>
	</out> 
      </action>
    </rule> 
    
    <rule comment="CHUNK: ger (canvia els ger que cal per de+inf)">
      <pattern>
	<pattern-item n="ger"/>
      </pattern>
      <action>
	<choose>
	  <when>
	    <test>
	      <equal>
		<clip pos="0" part="canvi"/>
		<lit-tag v="canviam"/>
	      </equal>
	    </test>
	    <let>
	      <clip pos="1" part="lem"/>
	      <lit v="de"/>
	    </let>
	    <out alt="oc">
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="lem"/>
		<clip pos="2" part="a_verb"/>
		<lit-tag v="inf"/>
	      </lu>
	    </out>   
	    <out alt="oc@aran">
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="3" part="lem"/>
		<clip pos="3" part="a_verb"/>
		<lit-tag v="inf"/>
	      </lu>
	    </out>   
	  </when>
	  <otherwise>
	    <out alt="oc">
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="whole"/>
	      </lu>
	    </out>
	    <out alt="oc@aran">
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="whole"/>
	      </lu>
	      <b pos="2"/>
	      <lu>
		<clip pos="3" part="whole"/>
	      </lu>
	    </out>
	  </otherwise>
	</choose>
      </action>
    </rule> 

