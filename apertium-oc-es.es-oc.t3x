  <section-def-macros>
    <def-macro n="cv_verb" npar="1">
      <choose>
	<when>
	  <test>
	    <equal>
	      <var n="quin_verb"/>
	      <lit-tag v="venir"/>
	    </equal>
	  </test>
	  <let>
	    <clip pos="1" part="lem"/>
	    <lit v="venir"/>
	  </let>
	</when>
	<when>
	  <test>
	    <equal>
	      <var n="quin_verb"/>
	      <lit-tag v="contunhar"/>
	    </equal>
	  </test>
	  <let>
	    <clip pos="1" part="lem"/>
	    <lit v="contunhar"/>
	  </let>
	</when>
      </choose> 
      <let>
	<var n="quin_verb"/>
	<lit-tag v=""/>
      </let>
    </def-macro>
    
    <def-macro n="f_bcond" npar="2">
      <choose>
	<when>
	  <test>
	    <not>
	      <equal>
		<b pos="1"/>   
		<lit v=" "/>
	      </equal>
	    </not>
	  </test>
	  <out>
	    <b pos="1"/>
	  </out>  
	</when>     
      </choose>
    </def-macro>
    
  </section-def-macros>

