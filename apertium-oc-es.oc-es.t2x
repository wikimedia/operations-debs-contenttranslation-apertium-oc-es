    </def-cat>

  
    <def-cat n="SA">
      <cat-item tags="SA.*"/>
    </def-cat>


    <def-cat n="DET">
      <cat-item tags="DET.*"/>
    </def-cat>
    
    <def-cat n="NUM">
      <cat-item tags="NUM.*"/>
