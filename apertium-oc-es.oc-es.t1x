    <def-macro n="cv_prep" npar="1">
 <choose> 
	<when>
	  <test>
	    <equal>
	      <clip pos="1" side="sl" part="lem"/>
	      <lit v="de"/>
	    </equal>
	  </test>
	  <choose>
	    <when>
